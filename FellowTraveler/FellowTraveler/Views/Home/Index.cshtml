@model DataService.Route

<div class="container" align="center">




    <form method="post">
        <div class="col-lg-6" align="right"><a href="#" id="var">ВОДИТЕЛЬ</a></div>
        <div class="col-lg-6" align="left"><a href="#" id="var">ПАССАЖИР</a></div>
        <div class="container-fluid" id="data">
            <div><input type="text" class="routename" placeholder="Название маршрута"></div>
            <div>
                <input class="from" type="text" placeholder="Откуда">
                <button id="transfer" type="button" class="glyphicon glyphicon-transfer"></button>
                <input type="text" class="where" placeholder="Куда">
            </div>
            <div class="col-lg-12">
                <div class="col-lg-4"> <input type="time" class="time" placeholder="Выезд"></div>
                <div class="col-lg-4"><input class="datepicker-here"  type="text"  placeholder="Дата"></div>
                <div class="col-lg-4 col-sm-12"> <input min="0" step="25" type="number" class="price" placeholder="Цена"></div>
            </div>
        </div>
        <div><button type="submit" class="save">Сохранить</button></div>
    </form>
    <div id="map" style="width: 820px; height: 600px"></div>
    <!-- создание контейнера, в котором размещена карта -->
